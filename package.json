{
  "name": "carbideui",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build carbideui --configuration production",
    "build:lib:watch": "ng build carbideui --watch --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=default --reporter=junit --outputFile=junit.xml",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"projects/carbideui/src/**/*.{ts,html}\"",
    "lint:eslint:fix": "eslint \"projects/carbideui/src/**/*.{ts,html}\" --fix",
    "lint:prettier": "prettier --check \"projects/carbideui/src/**/*.{ts,html,scss}\"",
    "lint:prettier:fix": "prettier --write \"projects/carbideui/src/**/*.{ts,html,scss}\"",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "audit:prod": "npm audit --omit=dev --audit-level=high",
    "audit:licenses": "npx license-checker --production --failOn \"GPL-2.0;GPL-3.0;AGPL-1.0;AGPL-3.0;SSPL-1.0\"",
    "storybook": "ng run carbideui:storybook --config-dir ./.storybook",
    "build-storybook": "ng run carbideui:build-storybook --config-dir ./.storybook",
    "precheck": "npm run build:lib && npm run test && npm run lint && npm run audit:prod && npm run audit:licenses",
    "commit": "czg",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0 || ^21.0.0",
    "@angular/compiler": "^20.0.0 || ^21.0.0",
    "@angular/core": "^20.0.0 || ^21.0.0",
    "@angular/forms": "^20.0.0 || ^21.0.0",
    "@angular/platform-browser": "^20.0.0 || ^21.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0 || ^21.0.0",
    "@angular/router": "^20.0.0 || ^21.0.0",
    "@carbon/charts": "^1.27.2",
    "@carbon/styles": "^1.98.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@angular-devkit/build-angular": "^20.0.0 || ^21.0.0",
    "@angular-eslint/eslint-plugin": "^21.2.0 || ^22.0.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0 || ^22.0.0",
    "@angular-eslint/template-parser": "^21.2.0 || ^22.0.0",
    "@angular/build": "^20.0.0 || ^21.0.0",
    "@angular/cli": "^20.0.0 || ^21.0.0",
    "@angular/compiler-cli": "^20.0.0 || ^21.0.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@compodoc/compodoc": "^1.1.30",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/npm": "^13.1.4",
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-themes": "^10.2.10",
    "@storybook/angular": "^10.2.10",
    "@testing-library/angular": "^19.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^3.2.4 || ^4.0.0",
    "@vitest/ui": "^3.2.4 || ^4.0.0",
    "axe-core": "^4.10.0",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "ng-packagr": "^20.0.0 || ^21.0.0",
    "prettier": "^3.8.1",
    "scss": "^0.2.4",
    "storybook": "^10.2.1",
    "typescript": "~5.9.2",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^3.2.4 || ^4.0.0",
    "vitest-axe": "^0.1.0",
    "zone.js": "^0.16.0"
  },
  "overrides": {
    "glob": "^13.0.1"
  }
}
